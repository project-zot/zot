{
    "distSpecVersion": "1.1.0",
    "storage": {
        "rootDirectory": "/tmp/zot",
        "dedupe": false,
        "storageDriver": {
            "name": "s3",
            "region": "REGION_NAME",
            "bucket": "BUGKET_NAME",
            "rootdirectory": "/ROOTDIR",
            "secure": true,
            "skipverify": false
        }
    },
    "http": {
        "address": "0.0.0.0",
        "port": "8080"
    },
    "log": {
        "level": "debug"
    },
    "extensions": {
        "sync": {
            "credentialsFile": "",
            "DownloadDir": "/tmp/zot",
            "registries": [
                {
                    "urls": [
                        "https://ACCOUNTID.dkr.ecr.REGION.amazonaws.com"
                    ],
                    "onDemand": true,
                    "maxRetries": 5,
                    "retryDelay": "2m",
                    "credentialHelper": "ecr"
                }
            ]
        }
    }
}