{
    "distSpecVersion": "1.1.0",
    "storage": {
        "dedupe": true,
        "gc": true,
        "rootDirectory": "/tmp/zot",
        "cacheDriver": {
            "name": "redis",
            "rootDir": "/tmp/zot/_redis",
            "url": "redis://localhost:6379"
        },
        "storageDriver": {
            "name": "s3",
            "rootdirectory": "/zot",
            "region": "us-east-2",
            "regionendpoint": "localhost:4566",
            "bucket": "zot-storage",
            "secure": false,
            "skipverify": false
        }
    },
    "http": {
        "address": "0.0.0.0",
        "port": "8484"
    },
    "log": {
        "level": "debug"
    },
    "extensions": {
      "ui": {
        "enable": true
      },
      "search": {
        "enable": true
      }
    }
}
