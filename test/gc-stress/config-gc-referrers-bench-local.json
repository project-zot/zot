{
    "distSpecVersion": "1.1.0",
    "storage": {
        "rootDirectory": "/tmp/zot/local",
        "gc": true,
        "gcDelay": "20s",
        "gcInterval": "1s",
        "retention": {
            "delay": "20s",
            "policies": [
                {
                    "repositories": ["**"],
                    "deleteReferrers": true
                }
            ]
        }
    },
    "http": {
        "address": "127.0.0.1",
        "port": "8080"
    },
    "log": {
        "level": "debug",
        "output": "/tmp/gc-referrers-bench-local.log"
    }
}
